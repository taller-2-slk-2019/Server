{
  "name": "server",
  "version": "1.0.0",
  "description": "slack taller 2",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "node src/app.js",
    "db:reset": "npx sequelize db:migrate:undo:all",
    "db:migrate": "npx sequelize db:migrate",
    "db": "npm run db:reset && npm run db:migrate",
    "pretest": "npx eslint src",
    "test": "export NODE_ENV=test || set NODE_ENV=test&& export LOGOPS_LEVEL=FATAL || set LOGOPS_LEVEL=FATAL&& npm run db && nyc mocha --colors --recursive",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [
    "taller2"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "express-query-boolean": "^2.0.0",
    "firebase-admin": "^7.2.0",
    "istanbul": "^0.4.5",
    "logops": "^2.1.0",
    "moment": "^2.24.0",
    "mysql2": "^1.6.5",
    "p-iteration": "^1.1.8",
    "pg": "^7.9.0",
    "pg-hstore": "^2.3.2",
    "raml2html": "^7.2.2",
    "rand-token": "^0.4.0",
    "sequelize": "^5.3.0",
    "sequelize-cli": "^5.4.0",
    "sha1": "^1.1.1"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.16.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.3",
    "eslint": "^5.15.3",
    "mocha": "^6.0.2",
    "mock-req-res": "^1.0.4",
    "nyc": "^13.3.0",
    "sinon": "^7.2.7",
    "sinon-chai": "^3.3.0"
  }
}
